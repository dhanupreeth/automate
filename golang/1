package main 

import  ( 
    "fmt"
        )


func ints() func() int {
    j:=4
    return func() int {
        for i:=0;i<j;i++ {
            return i+2
            }
            }
            }

func main() {
    ins:=ints()
    fmt.Println(ins())   
    fmt.Println(ins())   
    fmt.Println(ins())   
    fmt.Println(ins())   
    }
        
